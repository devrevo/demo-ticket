{% extends 'adminbase.html.twig' %}
{% block title %}Registration Tech{% endblock %}
{% block body %}
    <h4> Registration </h4>
     <h6>Account Information</h6>
    {{ form_start(formTech) }}
    {{ form_widget(formTech._token) }}
    {{ form_row(formTech.Nom) }}
    {{ form_row(formTech.prenom) }}
    {{ form_row(formTech.Telephone) }}

     <h6>User Information</h6>
    {{ form_start(formCompte) }}
    {{ form_widget(formCompte._token) }}
        {{ form_row(formCompte.email) }} 
        {{ form_row(formCompte.Username) }} 
        {{ form_row(formCompte.password) }} 
        {{ form_row(formCompte.confirm_password) }} 
        <div class="container-contact100-form-btn">
            <button class="contact100-form-btn">
                <span>
                    Submit
                    <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                </span>
            </button>
        </div>
    {{ form_end(formCompte) }}
    {{ form_end(formTech) }}
    
    
    
{% endblock %}